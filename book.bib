@article{carroll-92,
	abstract = {In this article I examine cognates, lexical items from different languages which are identified by bilinguals as somehow being 'the same thing'. Cognates have at least 4 essential properties: 1) they are always structural units; 2) they are words; 3) words paired may be but need not be semantically identical; 4) there is always some kind of formal resemblance between cognates. I argue that these properties can be explained in terms of a particular model of lexical activation and word recognition, namely the Cohort Model. I also take up the question of defining cognates for psycho linguistic purposes and argue against using traditional extensional definitions based on etymology and genetic relatedness. I argue for defining cognates in terms of their structural representations and the processes which activate and select them.},
	author = {Carroll, Susanne E.},
	bdsk-url-1 = {http://dx.doi.org/10.1177/026765839200800201},
	doi = {10.1177/026765839200800201},
	eprint = {http://slr.sagepub.com/cgi/reprint/8/2/93.pdf},
	journal = {Second Language Research},
	number = {2},
	pages = {93--119},
	title = {On cognates},
	volume = {8},
	year = {1992}
}

